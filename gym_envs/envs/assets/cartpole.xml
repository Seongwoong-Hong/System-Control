<mujoco model="cartpole">
  <compiler angle="radian" coordinate="local" inertiafromgeom="auto" />
	<default>
		<joint armature="0" limited="true"/>
		<geom contype="0" friction="0 0.0 0.0" rgba="0.7 0.7 0 1"/>
	</default>
	<option gravity="0 0 -9.81" integrator="RK4" timestep="0.0166"/>
	<size nstack="3000"/>
	<worldbody>
		<!--geom name="ground" type="plane" pos="0 0 0" /-->
		<body name="rail" pos="0 0 0">
			<geom name="rail" pos="0 0 0" rgba="0.3 0.3 0.7 1" size="4 0.015 0.015" type="box"/>
			<body name="cart" pos="0 0 0">
				<joint axis="1 0 0" limited="true" name="slider" pos="0 0 0" range="-4 4" type="slide"/>
				<geom name="cart" pos="0 0 0" size="0.1 0.125 0.1" type="box"/>
				<inertial pos="0 0 -0.03" mass="1.0" diaginertia="0.00854166667 0.00666667 0.008541666667"/>
				<body name="pole" pos="0 0 0.5">
					<joint axis="0 1 0" name="hinge" pos="0 0 -0.5" range="-2.0 2.0" type="hinge"/>
					<geom name="cpole" rgba="0 0.7 0.7 1" size="0.03 0.02 0.5" type="box"/>
					<inertial pos="0 0 -0.03" mass="1.0" diaginertia="0.0834666667 0.083633333 0.0004333333"/>
				</body>
			</body>
		</body>
	</worldbody>
	<actuator>
		<motor ctrllimited="true" ctrlrange="-1 1" gear="400" joint="slider" name="slide"/>
	</actuator>
</mujoco>