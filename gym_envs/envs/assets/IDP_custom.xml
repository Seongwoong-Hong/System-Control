<mujoco model="IDP_custom">
  <compiler angle="radian" coordinate="local" inertiafromgeom="false"/>
  <default>
    <joint damping="0.0"/>
    <geom contype="0" friction="0.0 0.0 0.0" rgba="0.7 0.7 0 1"/>
  </default>
  <option gravity="0 0 -9.81" integrator="RK4" timestep="0.001"/>
  <size nstack="3000" nuserdata="100" nuser_actuator="10"/>
  <worldbody>
    <geom name="floor" pos="0 0 -3.0" rgba="0.1 0.1 0.1 1" size="40 40 40" type="plane"/>
    <body name="leg" pos="0 0 0">
      <joint axis="0 1 0" limited="false" name="hinge" pos="0 0 0" type="hinge"/>
      <geom fromto="0 0 0 0 0 1.0" name="cpole" rgba="0 0.7 0.7 1" size="0.045" type="capsule"/>
      <inertial pos="0 0 0.5" mass="5.0" diaginertia="1.667 1.667 0.01"/>
      <body name="body" pos ="0 0 1.0">
        <joint axis="0 1 0" name="hinge2" pos="0 0 0" type="hinge"/>
        <geom fromto="0 0 0 0 0 1.0" name="cpole2" rgba="0 0.7 0.7 1" size="0.045" type="capsule"/>
        <inertial pos="0 0 0.5" mass="5.0" diaginertia="1.667 1.667 0.01"/>
      </body>
    </body>
  </worldbody>
  <actuator>
    <motor ctrllimited="true" ctrlrange='-1 1' gear="100" joint="hinge"/>
    <motor ctrllimited="true" ctrlrange='-1 1' gear="100" joint="hinge2"/>
  </actuator>
</mujoco>