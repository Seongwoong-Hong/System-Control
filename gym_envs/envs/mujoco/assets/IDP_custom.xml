<mujoco model="IDP_custom">
  <compiler angle="radian" coordinate="local" inertiafromgeom="false"/>
  <default>
    <joint damping="0.0"/>
    <geom contype="0" friction="0.0 0.0 0.0"/>
  </default>
  <option gravity="0 0 -9.81" integrator="RK4" timestep="0.001"/>
  <size nstack="3000" nuserdata="100" nuser_actuator="10"/>
  <worldbody>
    <geom name="floor" pos="0 0 -3" rgba="1 1 1 1" size="40 40 40" type="plane"/>
    <body name="leg" pos="0 0 0">
      <joint axis="0 1 0" limited="true" range="-0.5 0.5" name="hinge" pos="0 0 0" type="hinge"/>
      <geom fromto="0 0 0 0 0 1.0" name="cpole" rgba="0.9 0.4 0.0 0.7" size="0.045" type="capsule"/>
      <inertial pos="0 0 0.5" mass="5.0" diaginertia="0.41667 0.41667 0.0001"/>
      <body name="body" pos ="0 0 1.0">
        <joint axis="0 1 0" limited="true" range="-0.5 0.5" name="hinge2" pos="0 0 0" type="hinge"/>
        <geom fromto="0 0 0 0 0 1.0" name="cpole2" rgba="0.8 0.4 0.0 0.7" size="0.045" type="capsule"/>
        <inertial pos="0 0 0.5" mass="5.0" diaginertia="0.41667 0.41667 0.0001"/>
      </body>
    </body>
  </worldbody>
  <actuator>
    <motor ctrllimited="true" ctrlrange='-1 1' gear="200" joint="hinge"/>
    <motor ctrllimited="true" ctrlrange='-1 1' gear="200" joint="hinge2"/>
  </actuator>
</mujoco>